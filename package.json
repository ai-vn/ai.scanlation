{
  "name": "ai.scanlation",
  "version": "3.0.0",
  "license": "MIT",
  "description": "Scanlation tool for Ai Scanlation",
  "author": "Phạm Hồng Phúc - phamhongphuc.info@gmail.com",
  "scripts": {
    "dev": "electron-webpack dev",
    "build": "cross-env NODE_ENV=production nuxt generate && electron-webpack main && electron-builder",
    "build:dir": "yarn build --dir -c.compression=store -c.mac.identity=null",
    "prettier:fix": "prettier ** --write",
    "lint": "run-s lint:*",
    "lint:js": "eslint --ext .ts,.vue .",
    "lint:css": "stylelint **/*.{vue,scss}",
    "type:fix": "run-s prettier:fix \"lint:* --fix\""
  },
  "build": {
    "appId": "net.ai-vn.application"
  },
  "dependencies": {
    "nuxt": "^2.8.1",
    "nuxt-property-decorator": "^2.3.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0"
  },
  "devDependencies": {
    "@nuxt/config": "^2.8.1",
    "@nuxt/typescript": "^2.8.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "electron": "^5.0.8",
    "electron-builder": "^21.1.5",
    "electron-webpack": "^2.7.4",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.37.0"
  }
}
