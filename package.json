{
  "name": "ai.scanlation",
  "version": "3.0.7",
  "license": "MIT",
  "description": "Scanlation tool for Ai Scanlation",
  "author": "Phạm Hồng Phúc - phamhongphuc.info@gmail.com",
  "scripts": {
    "dev": "run-p dev:nuxt dev:electron",
    "dev:nuxt": "cross-env PORT=3000 NODE_ENV=development nuxt-ts",
    "dev:electron": "cross-env PORT=3000 NODE_ENV=development electron-webpack dev",
    "dev:security": "cross-env ELECTRON_SECURITY_CHECK=strict yarn dev",
    "build": "nuxt-ts build && electron-webpack main && electron-builder",
    "build-open": "yarn build --dir && yarn open",
    "open": "dist\\win-unpacked\\ai.scanlation.exe",
    "clean": "run-s clean:*",
    "clean:dist": "rimraf dist",
    "clean:key": "rimraf .certificate",
    "release": "yarn build --publish always",
    "postinstall": "electron-builder install-app-deps",
    "prettier:fix": "prettier ** --write",
    "lint": "run-s lint:*",
    "lint:js": "eslint --ext .ts,.vue .",
    "lint:css": "stylelint **/*.{vue,pcss}",
    "type:fix": "run-s prettier:fix \"lint:* --fix\"",
    "test": "jest"
  },
  "build": {
    "appId": "net.ai-vn.application",
    "nsis": {
      "oneClick": false,
      "perMachine": true
    },
    "win": {
      "target": [
        "portable",
        "nsis-web"
      ],
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.1.7",
    "electron-updater": "^4.1.2",
    "nuxt": "^2.9.2",
    "nuxt-property-decorator": "^2.4.0",
    "pem": "^1.14.3",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "v-tooltip": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-typescript": "^7.3.3",
    "@nuxt/typescript-build": "^0.2.10",
    "@nuxtjs/tailwindcss": "^1.1.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/pem": "^1.9.5",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.4.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.7",
    "electron-webpack": "^2.7.4",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "postcss-atroot": "^0.1.3",
    "postcss-each": "^0.10.0",
    "postcss-functions": "^3.0.0",
    "postcss-hexrgba": "^1.0.2",
    "postcss-mixins": "^6.2.2",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-simple-vars": "^5.0.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.2",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "webpack": "^4.41.0"
  }
}
