  "scripts": {
    "dev": "run-p dev:nuxt dev:electron",
    "dev:nuxt": "cross-env PORT=3000 NODE_ENV=development nuxt-ts",
    "dev:electron": "cross-env PORT=3000 NODE_ENV=development electron-webpack dev",
    "dev:security": "cross-env ELECTRON_SECURITY_CHECK=strict yarn dev",
    "build": "nuxt-ts build && electron-webpack main && electron-builder",
    "build:open": "yarn build --dir && yarn open",
    "open": "dist\\win-unpacked\\ai.scanlation.exe",
    "clean": "run-s clean:*",
    "clean:dist": "rimraf dist",
    "clean:key": "rimraf .certificate",
    "release": "yarn build --publish always",
    "postinstall": "electron-builder install-app-deps",
    "prettier:fix": "prettier ** --write",
    "lint": "run-s lint:*",
    "lint:js": "eslint --ext .ts,.vue .",
    "lint:css": "stylelint **/*.{vue,pcss}",
    "type:fix": "run-s prettier:fix \"lint:* --fix\"",
    "test": "jest"
  },
